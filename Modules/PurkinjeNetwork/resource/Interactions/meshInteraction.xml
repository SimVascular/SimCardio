<statemachine>

  <state name="Start" startstate="true">

    <transition event_class="MousePressEvent" event_variant="PrimaryButtonPressed" target="Start">
      <action    name="select_point"/>
    </transition>

    <transition event_class="InteractionKeyEvent" event_variant="KeyPressed_S" target="Start">
      <action name="add_start" />
    </transition>


  </state>


</statemachine>
